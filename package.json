{
  "name": "myapp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:dev": "expo start --web --port 8081",
    "web:test": "expo start --web --port 8081",
    "test": "node src/testing/MasterTestRunner.js",
    "test:auth": "node src/testing/MasterTestRunner.js --suite=auth",
    "test:cycle": "node src/testing/MasterTestRunner.js --suite=cycle",
    "test:ui": "node src/testing/MasterTestRunner.js --suite=ui",
    "test:playwright": "node src/testing/MasterTestRunner.js --suite=playwright",
    "test:verbose": "node src/testing/MasterTestRunner.js --verbose",
    "playwright:test": "playwright test",
    "playwright:headed": "playwright test --headed",
    "playwright:debug": "playwright test --debug",
    "playwright:report": "playwright show-report",
    "playwright:install": "playwright install",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "playwright test visual-regression.spec.js",
    "test:accessibility": "playwright test --grep accessibility",
    "test:performance": "playwright test --grep performance",
    "test:mobile": "playwright test --grep 'mobile viewport'",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "expo": "~53.0.22",
    "expo-apple-authentication": "^7.2.4",
    "expo-auth-session": "^6.2.1",
    "expo-barcode-scanner": "^13.0.1",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-notifications": "^0.31.4",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "^14.2.0",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-worklets": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.55.0",
    "playwright": "^1.55.0"
  },
